  "license": "MIT",
  "scripts": {
    "build:cli": "cd packages/hamburger-cli && pnpm build",
    "build:lib": "cd packages/hamburger-js && pnpm build && pnpm bundle",
    "build:dsl": "cd packages/hamburger-loader && pnpm build",
    "test:lib": "cd packages/hamburger-js && pnpm test",
    "dev:lib": "cd packages/hamburger-js && pnpm dev",
    "build": "pnpm build:cli && pnpm build:lib && pnpm build:cli && pnpm build:dsl",
    "test": "pnpm test",
    "dev": "pnpm dev:lib",
    "playground": "cd examples/basic && pnpm start",
    "deps": "taze major -w -r"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/plugin-transform-react-jsx": "^7.16.0",
