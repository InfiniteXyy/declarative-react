{
  "name": "hamburger",
  "version": "1.0.0",
  "main": "index.js",
  "private": true,
  "repository": "https://github.com/hamburger-js/hamburger",
  "workspaces": [
    "examples/*",
    "experiment",
    "packages/*"
  ],
