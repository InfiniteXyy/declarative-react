    "@babel/preset-flow": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@hamburger/core": "0.0.1",
    "@hamburger/loader": "0.0.1",
    "@hamburger/platform-react": "0.0.1",
    "@parcel/transformer-sass": "^2.0.0",
    "@types/jest": "^27.0.2",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "babel-jest": "^27.3.1",
    "babel-loader": "^8.2.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "jest": "^27.3.1",
    "parcel": "^2.0.0",
    "prettier": "^2.4.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rollup": "^2.58.3",
    "taze": "^0.4.5",
    "tslib": "^2.3.1",
    "typescript": "^4.4.4"
  }
}
